{
  "name": "cs484-final-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && cd functions && npm i",
    "update": "npm update && cd functions && npm update",
    "dev": "npm run build:functions && concurrently -k \"npm:emulators\" \"npm:vite\"",
    "emulators": "npx firebase-tools emulators:start --only auth,firestore,database,functions",
    "vite": "vite",
    "build": "npm run build:functions && tsc -b && vite build",
    "build:functions": "cd functions && npm run build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@vis.gl/react-google-maps": "^1.7.1",
    "firebase": "^12.6.0",
    "firebase-functions": "^7.0.0",
    "motion": "^12.23.25",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-icons": "^5.5.0",
    "react-router": "^7.9.6",
    "tailwindcss": "^4.1.17",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "firebase-tools": "^14.26.0",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.4"
  }
}
